        <result column="update_time" property="updateTime" />
        <result column="third_uid" property="thirdUid" />
        <result column="phone" property="phone" />
        <result column="email" property="email" />
        <result column="password" property="password" />
        <result column="avatar" property="avatar" />
        <result column="salt" property="salt" />
        <result column="nick_name" property="nickName" />
        <result column="bit_state" property="bitState" />
        <result column="sec_level" property="secLevel" />
        <result column="login_id" property="loginId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, create_time AS createTime, update_time AS updateTime, third_uid AS thirdUid, phone, email, password, avatar, salt, nick_name AS nickName, bit_state AS bitState, sec_level AS secLevel, login_id AS loginId
