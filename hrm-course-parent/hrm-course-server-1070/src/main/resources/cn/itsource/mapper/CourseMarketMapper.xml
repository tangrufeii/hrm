<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itsource.mapper.CourseMarketMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.itsource.domain.CourseMarket">
        <id column="id" property="id" />
        <result column="charge" property="charge" />
        <result column="expires" property="expires" />
        <result column="qq" property="qq" />
        <result column="price" property="price" />
        <result column="price_old" property="priceOld" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, charge, expires, qq, price, price_old AS priceOld
    </sql>

</mapper>
